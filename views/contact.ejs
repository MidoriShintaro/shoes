<!-- Contact Start -->
<div class="container-fluid">
    <h2 class="section-title position-relative text-uppercase mx-xl-5 mb-4"><span class="bg-secondary pr-3">Liên hệ với
            chúng tôi</span></h2>
    <div class="row px-xl-5">
        <div class="col-lg-7 mb-5">
            <div class="contact-form bg-light p-30">
                <div id="success"></div>
                <form action="/contact/sendfeedback" method="post">
                    <div class="control-group mt-2 mb-2">
                        <input type="text" class="form-control" name="name" id="name" placeholder="Nhập tên của bạn"
                            required="required" />
                    </div>
                    <div class="control-group mt-2 mb-2">
                        <input type="email" class="form-control" name="email" id="email"
                            placeholder="Nhập email của bạn" required="required" />
                    </div>
                    <div class="control-group mt-2 mb-2">
                        <input type="text" class="form-control" name="subject" id="subject" placeholder="Nhập tiêu đề"
                            required="required" />
                    </div>
                    <div class="control-group mt-2 mb-2">
                        <textarea class="form-control" rows="8" name="message" id="message"
                            placeholder="Nhập ý kiến của bạn" required="required"></textarea>
                    </div>
                    <div>
                        <button class="btn btn-primary py-2 px-4" type="submit" id="sendMessageButton">Gửi</button>
                    </div>
                </form>
            </div>
        </div>
        <div class="col-lg-5 mb-5">
            <div class="bg-light p-30 mb-30">
                <iframe
                    src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d7837.681431606026!2d106.6830715338966!3d10.823498952375322!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3174deb3ef536f31%3A0x8b7bb8b7c956157b!2zVHLGsOG7nW5nIMSQ4bqhaSBo4buNYyBDw7RuZyBuZ2hp4buHcCBUUC5IQ00!5e0!3m2!1svi!2s!4v1662564259299!5m2!1svi!2s"
                    width="100%" height="250px" style="border:0;" allowfullscreen="" loading="lazy"
                    referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>
            <div class="bg-light p-30 mb-3">
                <p class="mb-2"><i class="fa fa-map-marker-alt text-primary mr-3"></i>12 Nguyễn Văn Bảo, Phường 4, Gò
                    Vấp, TP.Hồ Chí Minh</p>
                <p class="mb-2"><i class="fa fa-envelope text-primary mr-3"></i>mizu12387@gmail.com</p>
                <p class="mb-2"><i class="fa fa-phone-alt text-primary mr-3"></i>+84 xxx692</p>
            </div>
        </div>
    </div>
</div>
<!-- Contact End -->